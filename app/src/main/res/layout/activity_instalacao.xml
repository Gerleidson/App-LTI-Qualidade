<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_gravity="center"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".instalacao">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="374dp"
        android:layout_height="714dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,UselessParent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="83dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/icon"
            tools:ignore="ContentDescription,ImageContrastCheck" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="Insira seus dados para agendamento"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="Nome completo | Empresa"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <EditText
            android:id="@+id/nomeEditText"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:background="@drawable/buttonshape"
            android:ems="10"
            android:inputType="text"
            android:padding="5dp"
            tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="Endereço / Nº"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <EditText
            android:id="@+id/enderecoEditText"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:background="@drawable/buttonshape"
            android:ems="10"
            android:inputType="text"
            android:padding="5dp"
            tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="177dp"
                android:layout_height="match_parent"
                android:gravity=""
                android:text="Bairro"
                android:textSize="14dp"
                tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

            <Space
                android:layout_width="21dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="186dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity=""
                android:text="Cidade"
                android:textSize="14sp"
                tools:ignore="HardcodedText,TextSizeCheck" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/bairroEditText"
                android:layout_width="177dp"
                android:layout_height="34dp"
                android:background="@drawable/buttonshape"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:padding="5dp"
                tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <Space
                android:layout_width="11dp"
                android:layout_height="match_parent" />

            <EditText
                android:id="@+id/cidadeEditText"
                android:layout_width="183dp"
                android:layout_height="34dp"
                android:background="@drawable/buttonshape"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:padding="5dp"
                tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="Documento`"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

            <EditText
                android:id="@+id/cpf"
                android:layout_width="236dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:ems="10"
                android:inputType="numberDecimal"
                android:padding="5dp"
                tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView35"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="É urgente?"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="OBS: Será cobrada uma taxa caso seja urgente"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <EditText
            android:id="@+id/urgenteEditText"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:background="@drawable/buttonshape"
            android:ems="10"
            android:inputType="text"
            android:padding="5dp"
            tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="Telefone para contato com DDD"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

        <EditText
            android:id="@+id/contato"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:background="@drawable/buttonshape"
            android:ems="10"
            android:inputType="phone"
            android:padding="5dp"
            tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity=""
            android:text="Descreva o aparelho para instalação:"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,TextSizeCheck" />

        <EditText
            android:id="@+id/anotacao"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:background="@drawable/buttonshape"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:padding="5dp"
            tools:ignore="Autofill,LabelFor,SpeakableTextPresentCheck" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="34dp"
                android:layout_weight="1"
                android:gravity=""
                android:text="Forma de pagamento"
                tools:ignore="HardcodedText" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="208dp"
                android:layout_height="wrap_content"
                tools:ignore="TouchTargetSizeCheck" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/shareButton"
            android:layout_width="76dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/LightBlue"
            android:clickable="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.958"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.403"
            app:srcCompat="@drawable/ic_share"
            tools:ignore="ContentDescription,KeyboardInaccessibleWidget,MissingConstraints,SpeakableTextPresentCheck" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
